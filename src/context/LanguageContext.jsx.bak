    const t = (key) => {
        const langData = translations[language] || translations['English'];
        const keys = key.split('.');
        let value = langData;
        let engValue = translations['English'];

        for (const k of keys) {
            value = value ? value[k] : undefined;
            engValue = engValue ? engValue[k] : undefined;
        }

        return value || engValue || key;
    };

